{
  "name": "blog.naturalclar.dev",
  "version": "0.1.0",
  "private": true,
  "description": "My Tech Blog, powered by Gatsby",
  "keywords": [
    "gatsby",
    "blog"
  ],
  "homepage": "https://blog.naturalclar.dev",
  "bugs": {
    "url": "https://github.com/Naturalclar/blog.naturalclar.dev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naturalclar/blog.naturalclar.dev.git"
  },
  "license": "MIT",
  "author": "Jesse Katsumata <jesse.katsumata@gmail.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern public .",
    "new": "scaffdog generate",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "gatsby": "^2.19.17",
    "gatsby-image": "^2.0.22",
    "gatsby-mdx": "^0.3.5",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-responsive-iframe": "^2.2.31",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.4.6",
    "mdx-utils": "^0.0.2",
    "prism-react-renderer": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-live": "^2.2.3",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^2.0.5",
    "scaffdog": "^0.2.3",
    "typescript": "^3.8.3"
  }
